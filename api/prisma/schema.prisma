generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id        Int    @id @default(autoincrement())
  username  String @db.VarChar(50)
  password  String @db.VarChar(100)
  full_name String @db.VarChar(100)
}
